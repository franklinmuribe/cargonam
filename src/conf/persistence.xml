<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<persistence xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.1" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="cargonamServicesPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>com.java.cargonam.bean.Role</class>
    <class>com.java.cargonam.bean.MenuTrace</class>
    <class>com.java.cargonam.bean.AccountRole</class>
    <class>com.java.cargonam.bean.EmailTypes</class>
    <class>com.java.cargonam.bean.ClientTypes</class>
    <class>com.java.cargonam.bean.Cities</class>
    <class>com.java.cargonam.bean.Countries</class>
    <class>com.java.cargonam.bean.Systems</class>
    <class>com.java.cargonam.bean.Sectors1</class>
    <class>com.java.cargonam.bean.Client</class>
    <class>com.java.cargonam.bean.PersonType</class>
    <class>com.java.cargonam.bean.PersonAddres</class>
    <class>com.java.cargonam.bean.PermissionRole</class>
    <class>com.java.cargonam.bean.Person</class>
    <class>com.java.cargonam.bean.SubActionTrace</class>
    <class>com.java.cargonam.bean.AgentPerson</class>
    <class>com.java.cargonam.bean.Sectors2</class>
    <class>com.java.cargonam.bean.Regions</class>
    <class>com.java.cargonam.bean.PersonPhones</class>
    <class>com.java.cargonam.bean.Account</class>
    <class>com.java.cargonam.bean.Agent</class>
    <class>com.java.cargonam.bean.AddressesTypes</class>
    <class>com.java.cargonam.bean.PhonesTypes</class>
    <class>com.java.cargonam.bean.AuditTrace</class>
    <class>com.java.cargonam.bean.PersonEmails</class>
    <class>com.java.cargonam.bean.Seller</class>
    <class>com.java.cargonam.bean.SellerPerson</class>
    <class>com.java.cargonam.bean.ActionTrace</class>
    <class>com.java.cargonam.bean.Permission</class>
    <class>com.java.cargonam.bean.DocumentType</class>
    <class>com.java.cargonam.bean.Phones</class>
    <class>com.java.cargonam.bean.Emails</class>
    <class>com.java.cargonam.bean.Addres</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/cargonam"/>
      <property name="javax.persistence.jdbc.user" value="postgres"/>
      <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
      <property name="javax.persistence.jdbc.password" value="cargonam"/>
      <property name="eclipselink.logging.level" value="FINE"/>
      <property name="eclipselink.logging.exceptions" value="true"/>
    </properties>
  </persistence-unit>
</persistence>
